    {
      "title": "Active Connections",
      "type": "timeseries",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "sum(pg_stat_activity_count{datname=\"flight_data\"}) by (state)",
          "legendFormat": "{{state}}"
        }
      ]
    },
    {
      "title": "Transactions per Second",
      "type": "timeseries",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "sum(rate(pg_stat_database_xact_commit{datname=\"flight_data\"}[5m]))",
          "legendFormat": "commits"
        },
        {
          "expr": "sum(rate(pg_stat_database_xact_rollback{datname=\"flight_data\"}[5m]))",
          "legendFormat": "rollbacks"
        }
      ],
      "unit": "tps"
    },
    {
      "title": "Cache Hit Rate",
      "type": "gauge",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "sum(rate(pg_stat_database_blks_hit{datname=\"flight_data\"}[5m])) / (sum(rate(pg_stat_database_blks_hit{datname=\"flight_data\"}[5m])) + sum(rate(pg_stat_database_blks_read{datname=\"flight_data\"}[5m]))) * 100",
          "legendFormat": "cache hit rate"
        }
      ],
      "unit": "percent"
    },
    {
      "title": "Rows Fetched vs. Returned",
      "type": "timeseries",
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "sum(rate(pg_stat_database_tup_fetched{datname=\"flight_data\"}[5m]))",
          "legendFormat": "fetched"
        },
        {
          "expr": "sum(rate(pg_stat_database_tup_returned{datname=\"flight_data\"}[5m]))",
          "legendFormat": "returned"
        }
      ]
    }
