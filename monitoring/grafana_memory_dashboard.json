{
  "dashboard": {
    "id": null,
    "title": "Flight Crawler - Memory Monitoring Dashboard",
    "tags": ["memory", "monitoring", "flight-crawler", "production"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "üìä Memory Usage (MB)",
        "type": "stat",
        "targets": [
          {
            "expr": "memory_usage_bytes{component=\"rss_mb\"} / 1024 / 1024",
            "legendFormat": "RSS Memory (MB)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1024},
                {"color": "red", "value": 2048}
              ]
            },
            "unit": "bytes"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "üìà Memory Usage Trend",
        "type": "graph",
        "targets": [
          {
            "expr": "memory_usage_bytes{component=\"rss_mb\"}",
            "legendFormat": "RSS Memory",
            "refId": "A"
          },
          {
            "expr": "memory_usage_bytes{component=\"vms_mb\"}",
            "legendFormat": "VMS Memory",
            "refId": "B"
          },
          {
            "expr": "memory_usage_bytes{component=\"available_mb\"}",
            "legendFormat": "Available Memory",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Memory (MB)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "‚ö†Ô∏è Memory Alerts",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(memory_alerts_sent_total)",
            "legendFormat": "Total Alerts",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 20}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 4,
        "title": "üîÑ Memory Percentage",
        "type": "gauge",
        "targets": [
          {
            "expr": "memory_usage_bytes{component=\"percent\"}",
            "legendFormat": "Memory %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 85}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "üíæ Swap Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "memory_usage_bytes{component=\"swap_percent\"}",
            "legendFormat": "Swap Usage %",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Swap %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 8}
      },
      {
        "id": 6,
        "title": "üóëÔ∏è Garbage Collection",
        "type": "stat",
        "targets": [
          {
            "expr": "memory_usage_bytes{component=\"gc_objects\"}",
            "legendFormat": "GC Objects",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100000},
                {"color": "red", "value": 500000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 8}
      },
      {
        "id": 7,
        "title": "üö® Alert Timeline",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"memory-monitoring\"}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "‚è±Ô∏è Check Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(memory_check_duration_seconds_bucket[5m]))",
            "legendFormat": "95th Percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(memory_check_duration_seconds_bucket[5m]))",
            "legendFormat": "50th Percentile",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (seconds)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 9,
        "title": "üîß Memory Cleanup Operations",
        "type": "stat",
        "targets": [
          {
            "expr": "memory_cleanup_operations_total",
            "legendFormat": "Total Cleanups",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "blue", "value": 10},
                {"color": "purple", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "hideControls": false,
    "links": [],
    "refresh": "30s",
    "schemaVersion": 16,
    "style": "dark",
    "templating": {
      "list": []
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "timezone": "",
    "version": 1
  }
} 